set terminal png size 1280,960
set output 'residuals.png'
set title "Data Monitoring"
set logscale y
set format y "%.0e"
set ylabel 'Residual'
set xlabel 'Time'
plot "postProcessing/residuals/2/residuals.dat" using 1:2 title 'p' with lines,\
     "postProcessing/residuals/2/residuals.dat" using 1:3 title 'Ux' with lines,\
     "postProcessing/residuals/2/residuals.dat" using 1:4 title 'Uy' with lines,\
     "postProcessing/residuals/2/residuals.dat" using 1:5 title 'Uz' with lines,\
     "postProcessing/residuals/2/residuals.dat" using 1:6 title 'k' with lines,\
     "postProcessing/residuals/2/residuals.dat" using 1:7 title 'epsilon' with lines
